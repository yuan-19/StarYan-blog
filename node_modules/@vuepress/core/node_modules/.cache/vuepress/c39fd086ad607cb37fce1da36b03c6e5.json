 isTel, ensureExt } from '../util'\n\nexport default {\n  props: {\n    item: {\n      required: true\n    }\n  },\n\n  computed: {\n    link () {\n      return ensureExt(this.item.link)\n    },\n\n    exact () {\n      if (this.$site.locales) {\n        return Object.keys(this.$site.locales).some(rootLink => rootLink === this.link)\n      }\n      return this.link === '/'\n    }\n  },\n\n  methods: {\n    isExternal,\n    isMailto,\n    isTel,\n    focusoutAction () {\n      this.$emit('focusout')\n    }\n  }\n}\n</script>\n"]}]}